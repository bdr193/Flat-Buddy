<%=  simple_form_for @search, url: rooms_path, validate: true, method: 'GET', :html => { :class => 'form-inline search-form' } do |f|  %>
  <%= f.input :city, name: 'city', value: '', placeholder: 'City', label: false %>
  <%= f.input :lat, as: :hidden %>
  <%= f.input :lng, as: :hidden %>
  <%= f.input :move_in_date, as: :string, input_html: { class: 'datepicker label-margin' },placeholder: 'Move in', label: false, required: true %>
  <%= f.input :move_out_date, as: :string, input_html: { class: 'datepicker label-margin' },placeholder: 'Move out (optional)', label: false %>
  <%= f.submit "Find FlatBuddies", class: "btn btn-success" %>
<% end %>
